<p id="notice"><%= notice %></p>

<h1>Rec Users</h1>

<table>
  <thead>
    <tr>
      <th>Object name</th>
      <th>Code</th>
      <th>User in out</th>
      <th>Rec comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rec_users.each do |rec_user| %>
      <tr>
        <td><%= rec_user.object_name %></td>
        <td><%= rec_user.code %></td>
        <td><%= rec_user.user_in_out %></td>
        <td><%= rec_user.rec_comment %></td>
        <td><%= link_to 'Show', rec_user %></td>
        <td><%= link_to 'Edit', edit_rec_user_path(rec_user) %></td>
        <td><%= link_to 'Destroy', rec_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rec User', new_rec_user_path %>
