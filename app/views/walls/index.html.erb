<p id="notice"><%= notice %></p>

<h1>Walls</h1>

<table>
  <thead>
    <tr>
      <th>Object name</th>
      <th>Multiplier</th>
      <th>Type 0</th>
      <th>Type env</th>
      <th>Length</th>
      <th>Pitch</th>
      <th>Area</th>
      <th>Orientation</th>
      <th>Construction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @walls.each do |wall| %>
      <tr>
        <td><%= wall.object_name %></td>
        <td><%= wall.multiplier %></td>
        <td><%= wall.type_0 %></td>
        <td><%= wall.type_env %></td>
        <td><%= wall.length %></td>
        <td><%= wall.pitch %></td>
        <td><%= wall.area %></td>
        <td><%= wall.orientation %></td>
        <td><%= wall.construction %></td>
        <td><%= link_to 'Show', wall %></td>
        <td><%= link_to 'Edit', edit_wall_path(wall) %></td>
        <td><%= link_to 'Destroy', wall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wall', new_wall_path %>
